<h1>Edit parameters</h1>
<br/>
<%=form_tag(admin_editparams_path, :method => "post")%>
<table class="table table-striped table-bordered table-condensed">
	<tr>
		<th>Name</th>
		<th>Value</th>
	</tr>
 <%  Constant.all.each do |c|%>
 	<tr>
 		<td><%= c.name%></td>
 		<td><input name="value_constants[<%= c.name%>][]" value="<%= c.value%>"/></td>
 	</tr>
 <% end%>
</table>
  <input type='submit' class="btn btn-primary" value="Save parameters">
<!--/form-->
</form>
