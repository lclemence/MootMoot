
<h1>Add pictures</h1>
<br /><br />

<%=form_tag(admin_upload_path, :method => "post", :enctype => 'multipart/form-data')%>
<h3>Select pictures you want to add</h3>
  <input name='uploads[]' type="file" multiple><br/>
  <input type='submit' class="btn btn-primary" value="Upload pictures">
<!--/form-->
</form>
