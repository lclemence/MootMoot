<h1>Upload pictures</h1>
<br /><br />
<!--form action='<%= upload_path%>' method='post' enctype='multipart/form-data'-->
<%=form_tag(upload_path, :method => "post", :enctype => 'multipart/form-data')%>
  <input name='uploads[]' type="file" multiple><br/>
  <input type='submit' class="btn btn-primary" value="Upload pictures">
<!--/form-->
</form>
<br/>
<br/>
<h1>Edit parameters</h1>
<br/>
<%=form_tag(editparams_path, :method => "post")%>
<table class="table table-striped table-bordered table-condensed">
	<tr>
		<th>Name</th>
		<th>Value</th>
	</tr>
 <%  Constant.all.each do |c|%>
 	<tr>
 		<td><%= c.name%></td>
 		<td><input name="value_constants[<%= c.name%>][]" value="<%= c.value%>"/></td>
 	</tr>
 <% end%>
</table>
  <input type='submit' class="btn btn-primary" value="Save parameters">
<!--/form-->
</form>
